x.com##div[data-testid^="UserAvatar-"] a div[style^="background-"]:style(border-radius:10px!important;)
x.com##div[data-testid^="UserAvatar-"] a div[style^="background-"] div:style(border-radius:0px!important;)
x.com##div[data-testid^="UserAvatar-"] a div[style^="background-"] div div[style^="background-image:"]:style(border-radius:0px!important;)
